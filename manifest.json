{
  "manifest_version": 3,
  "name": "BroTab",
  "short_name": "BroTab",
  "version": "1.4.0",
  "description": "Control your browser's tabs from command line",
  "key": "",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "brotab-icon-128x128.png"
  },
  "permissions": [
    "tabs",
    "nativeMessaging"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {}
}
